<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ScrollView
        android:id="@+id/scrollView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">


        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/profilePicImageView"
                android:layout_width="85dp"
                android:layout_height="85dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="20dp"
                tools:src="@color/black"
                app:layout_constraintHeight_default="percent"
                app:layout_constraintHeight_percent="0.8"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintWidth_default="percent"
                app:layout_constraintWidth_percent="0.2" />

            <TextView
                android:id="@+id/nameTextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="15dp"
                android:textColor="@color/black"
                android:textSize="22sp"
                android:text="에그"
                android:textStyle="bold"
                app:layout_constraintStart_toEndOf="@id/profilePicImageView"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/followsText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="6dp"
                android:text="팔로워"
                android:textColor="#838D95"
                android:textSize="12dp"
                app:layout_constraintStart_toStartOf="@id/nameTextView"
                app:layout_constraintTop_toBottomOf="@id/nameTextView" />

            <TextView
                android:id="@+id/followsTextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="5dp"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="6dp"
                android:text="0"
                android:textColor="#838D95"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="@id/followsText"
                app:layout_constraintStart_toEndOf="@id/followsText"
                app:layout_constraintTop_toTopOf="@id/followsText" />

            <TextView
                android:id="@+id/contourTextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:text="|"
                app:layout_constraintBottom_toBottomOf="@id/followsTextView"
                app:layout_constraintStart_toEndOf="@id/followsTextView"
                app:layout_constraintTop_toTopOf="@id/followsTextView" />

            <TextView
                android:id="@+id/followersText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="12dp"
                android:layout_marginTop="6dp"
                android:text="팔로잉"
                android:textColor="#838D95"
                android:textSize="12dp"
                app:layout_constraintStart_toEndOf="@id/contourTextView"
                app:layout_constraintTop_toBottomOf="@id/nameTextView" />

            <TextView
                android:id="@+id/followersTextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="5dp"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="6dp"
                android:text="0"
                android:textColor="#838D95"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="@id/followsText"
                app:layout_constraintStart_toEndOf="@id/followersText"
                app:layout_constraintTop_toTopOf="@id/followsText" />


            <RelativeLayout
                android:layout_width="170dp"
                android:layout_height="30dp"
                android:layout_marginTop="12dp"
                android:background="@drawable/edittext_rounded_corner_rectangle10"
                app:layout_constraintStart_toStartOf="@id/followsText"
                app:layout_constraintTop_toBottomOf="@id/followsTextView">


                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:paddingStart="10dp"
                    android:paddingEnd="10dp"
                    android:text="@string/friend"
                    android:textSize="12sp"
                    android:textStyle="bold" />

            </RelativeLayout>


            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/topConstraint"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="43dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/profilePicImageView">

                <ImageButton
                    android:id="@+id/my_pageButton1"
                    android:layout_width="27dp"
                    android:layout_height="27dp"
                    android:layout_marginStart="60dp"
                    android:background="@drawable/mypage1"
                    android:backgroundTint="#424242"
                    android:scaleType="fitXY"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <ImageButton
                    android:id="@+id/my_pageButton2"
                    android:layout_width="27dp"
                    android:layout_height="27dp"
                    android:layout_marginStart="105dp"
                    android:background="@drawable/mypage2bottom"
                    android:backgroundTint="#424242"
                    android:scaleType="fitXY"
                    app:layout_constraintStart_toEndOf="@id/my_pageButton1"
                    app:layout_constraintTop_toTopOf="parent" />

                <ImageButton
                    android:id="@+id/my_pageButton3"
                    android:layout_width="27dp"
                    android:layout_height="27dp"
                    android:layout_marginStart="105dp"
                    android:background="@drawable/mypage3"
                    android:backgroundTint="#424242"
                    android:scaleType="fitXY"
                    app:layout_constraintStart_toEndOf="@id/my_pageButton2"
                    app:layout_constraintTop_toTopOf="parent" />

            </androidx.constraintlayout.widget.ConstraintLayout>

            <TextView
                android:id="@+id/noticeTextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="60dp"
                android:layout_marginTop="3dp"
                android:text="알림"
                android:textColor="#747474"
                android:textSize="15sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/topConstraint" />

            <TextView
                android:id="@+id/scrapTextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="92dp"
                android:layout_marginTop="3dp"
                android:text="스크랩"
                android:textColor="#747474"
                android:textSize="15sp"
                app:layout_constraintStart_toEndOf="@id/noticeTextView"
                app:layout_constraintTop_toBottomOf="@id/topConstraint" />


            <TextView
                android:id="@+id/scrapNumberTextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="3dp"
                android:layout_marginTop="3dp"
                android:text="0"
                android:textColor="#747474"
                android:textSize="15sp"
                android:textStyle="bold"
                app:layout_constraintStart_toEndOf="@id/scrapTextView"
                app:layout_constraintTop_toBottomOf="@id/topConstraint" />

            <TextView
                android:id="@+id/likeTextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="76dp"
                android:layout_marginTop="3dp"
                android:text="좋아요"
                android:textColor="#747474"
                android:textSize="15sp"
                app:layout_constraintStart_toEndOf="@id/scrapNumberTextView"
                app:layout_constraintTop_toBottomOf="@id/topConstraint" />


            <TextView
                android:id="@+id/likeNumberTextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="3dp"
                android:layout_marginTop="3dp"
                android:text="0"
                android:textColor="#747474"
                android:textSize="15sp"
                android:textStyle="bold"
                app:layout_constraintStart_toEndOf="@id/likeTextView"
                app:layout_constraintTop_toBottomOf="@id/topConstraint" />

            <View
                android:id="@+id/view1"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="17dp"
                android:background="#E7E7E7"
                app:layout_constraintTop_toBottomOf="@id/likeNumberTextView" />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/myShoppingConstraintLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="15dp"
                android:layout_marginTop="17dp"
                android:layout_marginEnd="14dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHeight_default="percent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/view1"
                app:layout_constraintWidth_default="percent">


                <TextView

                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center|start"
                    android:text="나의 쇼핑"
                    android:textColor="@color/black"
                    android:textSize="17sp"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintHeight_default="percent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintWidth_default="percent" />

                <ImageButton
                    android:id="@+id/todayDillViewMore"
                    android:layout_width="22dp"
                    android:layout_height="13dp"
                    android:background="@android:color/transparent"
                    android:scaleType="fitCenter"
                    android:src="@drawable/rightarrow"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHeight_default="percent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:tint="#757575" />

            </androidx.constraintlayout.widget.ConstraintLayout>

            <RelativeLayout
                android:id="@+id/myShoppingRelativeLayout"
                android:layout_width="match_parent"
                android:layout_height="80dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="18dp"
                android:layout_marginEnd="16dp"
                android:background="@drawable/edittext_rounded_corner_rectangle11"
                app:layout_constraintStart_toStartOf="@id/myShoppingConstraintLayout"
                app:layout_constraintTop_toBottomOf="@id/myShoppingConstraintLayout">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">


                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/myShoppingConstraintLayout2"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginStart="33dp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent">

                        <TextView
                            android:id="@+id/orderTextView"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="25dp"
                            android:layout_marginTop="15dp"
                            android:gravity="center"
                            android:text="0"
                            android:textColor="#35C5F0"
                            android:textSize="18sp"
                            android:textStyle="bold"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="4dp"
                            android:gravity="center"
                            android:text="진행중 주문"
                            android:textColor="#424242"
                            android:textSize="13sp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@id/orderTextView" />


                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <TextView
                        android:id="@+id/myShoppingContourTextView"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginStart="30dp"
                        android:gravity="center"
                        android:text="|"
                        android:textColor="#DADCE0"
                        android:textStyle="bold"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toEndOf="@id/myShoppingConstraintLayout2"
                        app:layout_constraintTop_toTopOf="parent" />

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/myShoppingConstraintLayout3"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginStart="33dp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toEndOf="@id/myShoppingContourTextView"
                        app:layout_constraintTop_toTopOf="parent">

                        <TextView
                            android:id="@+id/myCouponTextView"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="25dp"
                            android:layout_marginTop="15dp"
                            android:gravity="center"
                            android:text="0"
                            android:textColor="#35C5F0"
                            android:textSize="18sp"
                            android:textStyle="bold"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="10dp"
                            android:layout_marginTop="4dp"
                            android:gravity="center"
                            android:text="내 쿠폰"
                            android:textColor="#424242"
                            android:textSize="13sp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@id/myCouponTextView" />


                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <TextView
                        android:id="@+id/myShoppingContourTextView2"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginStart="40dp"
                        android:gravity="center"
                        android:text="|"
                        android:textColor="#DADCE0"
                        android:textStyle="bold"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toEndOf="@id/myShoppingConstraintLayout3"
                        app:layout_constraintTop_toTopOf="parent" />

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/myShoppingConstraintLayout4"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginStart="33dp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toEndOf="@id/myShoppingContourTextView2"
                        app:layout_constraintTop_toTopOf="parent">

                        <TextView
                            android:id="@+id/myPointTextView"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="20dp"
                            android:layout_marginTop="15dp"
                            android:gravity="center"
                            android:text="0"
                            android:textColor="#35C5F0"
                            android:textSize="18sp"
                            android:textStyle="bold"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="7dp"
                            android:layout_marginTop="4dp"
                            android:gravity="center"
                            android:text="내 포인트"
                            android:textColor="#424242"
                            android:textSize="13sp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@id/myPointTextView" />


                    </androidx.constraintlayout.widget.ConstraintLayout>
                </androidx.constraintlayout.widget.ConstraintLayout>
            </RelativeLayout>

            <View
                android:id="@+id/view2"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="17dp"
                android:background="#E7E7E7"
                app:layout_constraintTop_toBottomOf="@id/myShoppingRelativeLayout" />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/photoConstraintLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="15dp"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="15dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHeight_default="percent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/view2"
                app:layout_constraintWidth_default="percent">


                <TextView
                    android:id="@+id/photoTextview"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center|start"
                    android:text="사진"
                    android:textColor="@color/black"
                    android:textSize="17sp"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintHeight_default="percent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/firstPhotoButton"
                    android:layout_width="176dp"
                    android:layout_height="22dp"
                    android:layout_marginStart="10dp"
                    android:layout_marginBottom="3dp"
                    android:background="@drawable/edittext_rounded_corner_rectangle12"
                    android:paddingStart="9dp"
                    android:paddingEnd="9dp"
                    android:text="첫 사진을 올리면 + 1,000p"
                    android:textColor="@color/white"
                    android:textSize="13sp"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="@id/photoTextview"
                    app:layout_constraintStart_toEndOf="@id/photoTextview"
                    app:layout_constraintTop_toTopOf="@id/photoTextview" />


            </androidx.constraintlayout.widget.ConstraintLayout>


            <TextView
                android:id="@+id/myHomeChangeTextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:text="우리 집의 변화를 공간별로 기록하고 모아볼 수 있어요"
                android:textColor="#939393"
                android:textSize="15sp"
                app:layout_constraintStart_toStartOf="@id/photoConstraintLayout"
                app:layout_constraintTop_toBottomOf="@id/photoConstraintLayout" />

            <ImageView
                android:id="@+id/photoRecordImageView"
                android:layout_width="match_parent"
                android:layout_height="380dp"
                android:layout_marginTop="20dp"
                android:layout_marginBottom="20dp"
                android:background="@drawable/photo"
                app:layout_constraintEnd_toEndOf="@id/photoConstraintLayout"
                app:layout_constraintStart_toStartOf="@id/photoConstraintLayout"
                app:layout_constraintTop_toBottomOf="@id/myHomeChangeTextView" />

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/appCompatImageView"
                android:layout_width="match_parent"
                android:layout_height="200dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/photobutton"
                android:paddingTop="20dp"
                app:layout_constraintEnd_toEndOf="@id/photoRecordImageView"
                app:layout_constraintStart_toStartOf="@id/photoRecordImageView"
                app:layout_constraintTop_toBottomOf="@id/photoRecordImageView" />

            <View
                android:id="@+id/view3"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="17dp"
                android:background="#E7E7E7"
                app:layout_constraintTop_toBottomOf="@id/appCompatImageView" />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/housesConstraintLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="15dp"
                android:layout_marginTop="17dp"
                android:layout_marginEnd="14dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHeight_default="percent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/view3"
                app:layout_constraintWidth_default="percent">


                <TextView

                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center|start"
                    android:text="집들이"
                    android:textColor="@color/black"
                    android:textSize="17sp"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintHeight_default="percent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintWidth_default="percent" />

                <ImageButton
                    android:id="@+id/housesViewMore"
                    android:layout_width="22dp"
                    android:layout_height="13dp"
                    android:background="@android:color/transparent"
                    android:scaleType="fitCenter"
                    android:src="@drawable/rightarrow"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHeight_default="percent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:tint="#757575" />

            </androidx.constraintlayout.widget.ConstraintLayout>

            <View
                android:id="@+id/view4"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="17dp"
                android:background="#E7E7E7"
                app:layout_constraintTop_toBottomOf="@id/housesConstraintLayout" />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/knowHowConstraintLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="15dp"
                android:layout_marginTop="17dp"
                android:layout_marginEnd="14dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHeight_default="percent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/view4"
                app:layout_constraintWidth_default="percent">


                <TextView

                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center|start"
                    android:text="노하우"
                    android:textColor="@color/black"
                    android:textSize="17sp"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintHeight_default="percent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintWidth_default="percent" />

                <ImageButton
                    android:id="@+id/knowHowViewMore"
                    android:layout_width="22dp"
                    android:layout_height="13dp"
                    android:background="@android:color/transparent"
                    android:scaleType="fitCenter"
                    android:src="@drawable/rightarrow"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHeight_default="percent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:tint="#757575" />

            </androidx.constraintlayout.widget.ConstraintLayout>

            <View
                android:id="@+id/view5"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="17dp"
                android:background="#E7E7E7"
                app:layout_constraintTop_toBottomOf="@id/knowHowConstraintLayout" />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/QnAConstraintLayout"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="15dp"
                android:layout_marginTop="17dp"
                android:layout_marginEnd="14dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHeight_default="percent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/view5"
                app:layout_constraintWidth_default="percent">


                <TextView
                    android:id="@+id/QnATextView"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center|start"
                    android:text="질문과 답변"
                    android:textColor="@color/black"
                    android:textSize="17sp"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintHeight_default="percent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
